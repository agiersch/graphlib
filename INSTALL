#!/usr/bin/make -f

DEST = $(HOME)/CoursInfo/Algo_S1/tp/graph

SRC = DrawingWindow.h DrawingWindow.cpp
SRC += exemple.cpp exemple.pro
SRC += index.html

.PHONY: install

install:
	doxygen Doxyfile
	install -m 755 -d $(DEST)
	install -m 644 $(SRC) $(DEST)
	install -m 755 -d $(DEST)/html
	install -m 644 html/* $(DEST)/html
